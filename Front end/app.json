{
  "expo": {
    "name": "Lavadero App",
    "slug": "lavadero-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lavadero.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4A90E2"
      },
      "package": "com.lavadero.app",
      "usesCleartextTraffic": true
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "cc8f5cd9-739c-4425-be4f-3e7a801e26af"
      }
    },
    "plugins": [
      ["expo-build-properties", { "android": { "usesCleartextTraffic": true } }],
      ["expo-navigation-bar", { "backgroundColor": "#ffffff", "barStyle": "dark", "position": "relative" }]
    ]
  }
}
